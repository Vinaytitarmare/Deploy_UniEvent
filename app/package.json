{
  "name": "centralized-event-platform",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "patch-sea": "node -e \"const fs=require('fs'),p='node_modules/@expo/cli/build/src/start/server/metro/externals.js';let c=fs.readFileSync(p,'utf8');if(!c.includes('PATCHED_SEA')){c=c.replace(/for\\s*\\(const\\s+(\\w+)\\s+of\\s+NODE_STDLIB_MODULES\\)/g,'for(const $1 of NODE_STDLIB_MODULES.filter(m=>!m.includes(\\\":\\\")))/*PATCHED_SEA*/');fs.writeFileSync(p,c);console.log('Patched!');}else{console.log('Already patched.');}\"",
    "prestart": "npm run patch-sea",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "npm run patch-sea && expo start --web",
    "build:web": "expo export -p web --output-dir web-build",
    "postinstall": "npm run patch-sea"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "^19.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/datetimepicker": "7.2.0",
    "@react-native-picker/picker": "2.4.10",
    "@react-navigation/bottom-tabs": "^6.0.0",
    "@react-navigation/native": "^6.0.0",
    "@react-navigation/stack": "^6.0.0",
    "expo": "~49.0.0",
    "expo-auth-session": "~5.0.2",
    "expo-barcode-scanner": "~12.5.3",
    "expo-blur": "~12.4.1",
    "expo-camera": "~13.4.4",
    "expo-crypto": "~12.4.1",
    "expo-device": "~5.4.0",
    "expo-image-picker": "~14.3.2",
    "expo-linear-gradient": "~12.3.0",
    "expo-linking": "~5.0.0",
    "expo-navigation-bar": "~2.3.0",
    "expo-notifications": "~0.20.1",
    "expo-print": "~12.4.2",
    "expo-secure-store": "~12.3.1",
    "expo-sharing": "~11.5.0",
    "expo-status-bar": "~1.6.0",
    "expo-web-browser": "~12.3.2",
    "firebase": "^10.0.0",
    "generator-function": "^2.0.1",
    "html5-qrcode": "^2.3.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.10",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-qrcode-svg": "^6.3.21",
    "react-native-ratings": "^8.1.0",
    "react-native-reanimated-carousel": "^4.0.3",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-web": "~0.19.6",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
